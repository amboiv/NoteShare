<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= @page_title %> </title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <meta name="keywords" content="<%= @seo_keywords %>"/>

    <%= stylesheet_link_tag    'notes', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  
     
    
    <main role="main">
    <div class="container">
      <%= render 'shared/notes_navbar' %>
    </div>
     
      <%= render 'shared/courses_masthead' unless logged_in?(:instructor) %>
      <%= yield %>
    </main>

    <%= render 'shared/courses_footer' %>
    <%= alerts %>
    

  </body>
</html>
