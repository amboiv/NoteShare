<tr>
    <td><%= note.title %></td>
    <td><%= note.body %></td>
    <td><%= link_to note.status, toggle_status_note_path(note) %></td>
    <td><%= link_to 'Show', note if logged_in?(:student, :instructor) %></td>
    <td><%= link_to 'Edit', edit_note_path(note) if logged_in?(:student, :instructor) %></td>
    <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } if logged_in?(:student) %></td>
</tr>