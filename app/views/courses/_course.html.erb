    <p><%= link_to course.name, course_show_path(course.id) %></p>
    <p><%= course.code %></p>
    <p><%= course.credits %></p>
    <p><%= course.description %></p>
    <%= image_tag course.thumbnail unless course.thumbnail.nil? %>
